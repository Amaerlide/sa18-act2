<h1>Products</h1>
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Price</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        <% @products.each do |product|%>
            <tr>
                <td><%= product.name %></td>
                <td>$<%= product.price %></td>
          
            </tr>
        <% end %>
    </tbody>
</table>